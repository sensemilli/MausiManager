    <SolidColorBrush x:Key="BrushBackground" Color="WhiteSmoke" />
    <Style x:Key="StyleTextBlock" TargetType="{x:Type TextBlock}">
        <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        <Setter Property="FrameworkElement.VerticalAlignment" Value="Center" />
        <Setter Property="FrameworkElement.MinHeight" Value="21" />
        <Setter Property="TextBlock.FontSize" Value="{Binding FontSize, FallbackValue=12}" />
    </Style>
    <Style x:Key="StyleTextBlockNumeric" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource StyleTextBlock}">
        <Setter Property="FrameworkElement.Width" Value="40" />
    </Style>
    <Style x:Key="StyleTextBlockShadow" TargetType="{x:Type TextBlock}" BasedOn="{StaticResource StyleTextBlock}">
        <Setter Property="FrameworkElement.Margin" Value="0,0,3,0" />
    </Style>
    <Style x:Key="StyleTextBoxNumeric" TargetType="{x:Type TextBox}">
        <Setter Property="FrameworkElement.VerticalAlignment" Value="Center" />
        <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        <Setter Property="FrameworkElement.Width" Value="60" />
        <Setter Property="Control.FontSize" Value="{Binding FontSize, FallbackValue=12}" />
    </Style>
    <Style x:Key="StyleTextBox" TargetType="{x:Type TextBox}">
        <Setter Property="FrameworkElement.VerticalAlignment" Value="Center" />
        <Setter Property="FrameworkElement.HorizontalAlignment" Value="Left" />
        <Setter Property="FrameworkElement.Margin" Value="0,0,10,2" />
        <Setter Property="FrameworkElement.MinWidth" Value="180" />
        <Setter Property="FrameworkElement.MinHeight" Value="21" />
        <Setter Property="Control.FontSize" Value="{Binding FontSize, FallbackValue=12}" />
    </Style>
