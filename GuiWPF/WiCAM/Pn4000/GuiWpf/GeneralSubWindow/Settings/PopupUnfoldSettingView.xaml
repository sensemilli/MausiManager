              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_ObjExportModeUnfold}" IsChecked="{Binding general3DConfig.P3D_ObjExportModeUnfoldModel}" />
              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_ObjExportModeModified}" IsChecked="{Binding general3DConfig.P3D_ObjExportModeModifiedModel}" />
              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_ObjExportModeReconstructed}" IsChecked="{Binding general3DConfig.P3D_ObjExportModeReconstructedModel}" />
              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_GlbExportModeOriginal}" IsChecked="{Binding general3DConfig.P3D_GlbExportModel}" />
              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_GlbExportModeUnfold}" IsChecked="{Binding general3DConfig.P3D_GlbExportModeUnfoldModel}" />
              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_GlbExportModeModified}" IsChecked="{Binding general3DConfig.P3D_GlbExportModeModifiedModel}" />
              <CheckBox Content="{DynamicResource l_popup.PopupUnfoldSetting.P3D_GlbExportModeReconstructed}" IsChecked="{Binding general3DConfig.P3D_GlbExportModeReconstructedModel}" />
