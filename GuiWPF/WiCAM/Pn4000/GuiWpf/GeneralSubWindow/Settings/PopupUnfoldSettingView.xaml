            <TextBox Grid.Column="2" Text="{Binding MaxDistOfPurchasePartsToSheetMetalUI}" />
            <Label Grid.Column="3" Content="{Binding MaxDistOfPurchasePartsToSheetMetalUnit}" />
          </Grid>
        </Grid>
