                  <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <TextBlock Style="{DynamicResource PnPopupFreeText}" Text="{DynamicResource l_popup.PopupUnfoldSetting.TotalTolerance}" />
              </Grid>
            </StackPanel>
            <TextBlock Style="{DynamicResource PnPopupFreeText}" Text="{DynamicResource l_popup.PopupUnfoldSetting.BendOptions}" />
            <StackPanel Orientation="Vertical" Margin="20,5,0,5">
              <Grid Margin="0,0,0,1">
                <CheckBox Style="{DynamicResource PnPopupCheckBox}" Content="{DynamicResource l_popup.PopupUnfoldSetting.IgnoreSelfCollision}" IsChecked="{Binding general3DConfig.P3D_IgnoreSelfCollision}" />
              </Grid>
            </StackPanel>
            <TextBlock Style="{DynamicResource PnPopupFreeText}" Text="{DynamicResource l_popup.PopupUnfoldSetting.StepBendOptions}" />
            <StackPanel Orientation="Vertical" Margin="20,5,0,5">
              <Grid Margin="0,0,0,1">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="3*" />
                  <ColumnDefinition Width="1*" />
                  <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <Label Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Column="0" Content="{DynamicResource l_popup.PopupUnfoldSetting.MinStepBendRadius}" />
                <src1:TextBoxDouble Grid.Column="1" Style="{DynamicResource PnPopupTextBoxStyle}" Min="0.0" Height="26" VerticalAlignment="Top" Text="{Binding general3DConfig.P3D_MinStepBendRadius}" />
                <Label Content="" Grid.Column="2" />
              </Grid>
              <Grid Margin="0,0,0,1">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="3*" />
                  <ColumnDefinition Width="1*" />
                  <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <Label Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Column="0" Content="{DynamicResource l_popup.PopupUnfoldSetting.MaxStepBendRadius}" />
                <src1:TextBoxDouble Grid.Column="1" Style="{DynamicResource PnPopupTextBoxStyle}" Min="0.0" Height="26" VerticalAlignment="Top" Text="{Binding general3DConfig.P3D_MaxStepBendRadius}" />
              </Grid>
              <Grid>
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="3*" />
                  <ColumnDefinition Width="1*" />
                  <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <Label Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Column="0" Content="{DynamicResource l_popup.PopupUnfoldSetting.WithoutMachineRadiusFactor}" />
                <src1:TextBoxDouble Grid.Column="1" Style="{DynamicResource PnPopupTextBoxStyle}" Min="0.0" Height="26" VerticalAlignment="Top" Text="{Binding general3DConfig.P3D_StepBendWithoutMachineRadiusFactor}" />
                <Label Content="{DynamicResource l_popup.PopupUnfoldSetting.RadiusToleranceThicknessAddOn}" Grid.Column="2" />
              </Grid>
              <Grid>
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="3*" />
                  <ColumnDefinition Width="1*" />
                  <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <Label Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Column="0" Content="{DynamicResource l_popup.PopupUnfoldSetting.WithoutMachineBendLengthFactor}" />
                <src1:TextBoxDouble Grid.Column="1" Style="{DynamicResource PnPopupTextBoxStyle}" Min="0.0" Height="26" VerticalAlignment="Top" Text="{Binding general3DConfig.P3D_StepBendWithoutMachineMinBendLengthFactor}" />
                <Label Content="{DynamicResource l_popup.PopupUnfoldSetting.RadiusToleranceThicknessAddOn}" Grid.Column="2" />
              </Grid>
              <Grid Margin="0,0,0,1">
                <CheckBox Style="{DynamicResource PnPopupCheckBox}" Content="{DynamicResource l_popup.PopupUnfoldSetting.PreferWaveToolOverStepBend}" IsChecked="{Binding general3DConfig.P3D_PreferWaveToolOverStepBend}" />
              </Grid>
            </StackPanel>
            <TextBlock Style="{DynamicResource PnPopupFreeText}" Text="{DynamicResource l_popup.PopupUnfoldSetting.ToolValidationOptions}" />
            <StackPanel Orientation="Vertical" Margin="20,5,0,5">
              <Grid Margin="0,0,0,1" ToolTip="{DynamicResource l_popup.PopupUnfoldSetting.ToolSafetyDistanceOptionTt}">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="1*" />
                  <ColumnDefinition Width="1*" />
                  <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <Label Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Column="0" Content="{DynamicResource l_popup.PopupUnfoldSetting.ToolSafetyDistanceOption}" />
                <src1:TextBoxDouble Grid.Column="1" Style="{DynamicResource PnPopupTextBoxStyle}" Min="0.0" Height="26" VerticalAlignment="Top" Text="{Binding simValidationConfig.SafetyDistance}" />
                <Label Content="{DynamicResource l_popup.PopupUnfoldSetting.ToolSafetyDistanceOptionSuffix}" Grid.Column="2" />
              </Grid>
            </StackPanel>
          </StackPanel>
        </ScrollViewer>
      </TabItem>
      <TabItem Header="{DynamicResource l_popup.PnInterfaceSettings.Legend3d}" Padding="15">
        <Grid>
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="1*" />
          </Grid.RowDefinitions>
          <WrapPanel Grid.Row="0">
            <CheckBox Content="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.CheckBeforeSave}" ToolTip="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.CheckBeforeSaveTt}" IsChecked="{Binding LegendCheckBeforeSave}" />
            <CheckBox Content="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.CheckBeforePp}" ToolTip="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.CheckBeforePpTt}" IsChecked="{Binding LegendCheckBeforePp}" />
          </WrapPanel>
          <gridView:PnGridViewContainer Grid.Row="1">
            <gridView:PnGridViewContainer.GridView>
              <telerik:RadGridView AutoGenerateColumns="False" NewRowPosition="Top" RightFrozenColumnCount="1" RightFrozenColumnsSplitterVisibility="Visible" ItemsSource="{Binding LegendEntries}">
                <telerik:GridViewDataControl.Columns>
                  <telerik:GridViewDataColumn Header="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.No}" DataMemberBinding="{Binding No}" />
                  <telerik:GridViewDataColumn Header="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.Desc}" DataMemberBinding="{Binding Desc}" />
                  <telerik:GridViewCheckBoxColumn Header="{DynamicResource l_popup.PnInterfaceSettings.Legend3d.Required}" EditTriggers="CellClick" AutoSelectOnEdit="True" DataMemberBinding="{Binding Required}" />
                  <telerik:GridViewDataColumn>
                    <telerik:GridViewColumn.CellTemplate>
                      <DataTemplate>
                                                <telerik:RadButton FrameworkElement.Cursor="Hand" Control.Foreground="#FFFF0000" ButtonBase.Command="{Binding Path=DataContext.CmdDeleteRow, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type telerik:RadGridView}}}" ButtonBase.CommandParameter="{Binding}" >
                                                    <telerik:RadGlyph telerik:RadGlyph.Glyph="" />
                        </telerik:RadButton>
                      </DataTemplate>
                    </telerik:GridViewColumn.CellTemplate>
                  </telerik:GridViewDataColumn>
                </telerik:GridViewDataControl.Columns>
              </telerik:RadGridView>
            </gridView:PnGridViewContainer.GridView>
          </gridView:PnGridViewContainer>
        </Grid>
      </TabItem>
      <TabItem Header="{DynamicResource l_popup.PopupUnfoldSetting.MacroConfig}" Padding="15">
        <local:MacroConfigView DataContext="{Binding MacroConfigViewModel}" />
      </TabItem>
    </TabControl>
  </Grid>
</src1:PopupBase>