                </telerik:GridViewBoundColumnBase.EditorStyle>
              </telerik:GridViewComboBoxColumn>
              <telerik:GridViewComboBoxColumn Header="{DynamicResource l_popup.PopupUnfoldSetting.Type}" DisplayMemberPath="Desc" SelectedValueMemberPath="Index" EditTriggers="CellClick,Default" DataMemberBinding="{Binding Type}" ItemsSource="{Binding PurchasedPartTypeList}">
