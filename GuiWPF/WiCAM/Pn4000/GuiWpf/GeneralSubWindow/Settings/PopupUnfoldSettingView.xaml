                      <controls:RadGlyph controls:RadGlyph.Glyph="" />
                    </controls:RadButton>
                  </DataTemplate>
                </telerik:GridViewColumn.CellTemplate>
              </telerik:GridViewDataColumn>
            </telerik:GridViewDataControl.Columns>
          </telerik:RadGridView>
          <StackPanel Grid.Row="0" Grid.Column="1" Orientation="Horizontal">
            <Label VerticalContentAlignment="Center" Content="{DynamicResource l_popup.PopupUnfoldSetting.PurchasedPartType}" />
            <Button ToolTip="{DynamicResource l_popup.PopupUnfoldSetting.PurchasedPartTypeNew}" Command="{Binding CmdAddNewPurchasedPartType}">
              <RadGlyph p21:RadGlyph.Glyph="" p21:RadGlyph.Foreground="#FF000000" p21:RadGlyph.FontSize="16" xmlns:p21="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls" xmlns="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls" />
            </Button>
          </StackPanel>
          <ScrollViewer Grid.Row="1" Grid.Column="1" VerticalScrollBarVisibility="Auto">
            <ItemsControl ItemsSource="{Binding PurchasedPartTypeList}">
