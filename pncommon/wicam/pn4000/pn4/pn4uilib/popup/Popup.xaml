<?xml version="1.0" encoding="utf-8"?>
<Window x:Class="WiCAM.Pn4000.pn4.pn4UILib.Popup.Popup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:WiCAM.Pn4000.pn4.pn4UILib.Popup"
        xmlns:tree="clr-namespace:WiCAM.Pn4000.WpfControls.ArchivesControl;assembly=WiCAM.Pn4000.Comon" 
        Background="{DynamicResource PnPopup.WindowBrush}" Title="popup" 
        Height="300" Width="809" ShowInTaskbar="False" Topmost="False" Visibility="Visible"
        WindowStartupLocation="Manual" MinHeight="300" MinWidth="400" Closing="Window_Closing" PreviewMouseRightButtonDown="Window_PreviewMouseRightButtonDown" PreviewMouseRightButtonUp="Window_PreviewMouseRightButtonUp" PreviewKeyDown="Window_PreviewKeyDown" SizeChanged="Window_SizeChanged" Loaded="Window_Loaded" IsVisibleChanged="Window_IsVisibleChanged" PreviewMouseLeftButtonDown="Window_PreviewMouseLeftButtonDown" LocationChanged="Window_LocationChanged" LostFocus="Window_LostFocus" Deactivated="Window_Deactivated" Activated="Window_Activated">
  <FrameworkElement.Resources>
    <ResourceDictionary>
      <Style x:Key="NavigationButtonStyle" TargetType="{x:Type Button}">
        <Setter Property="Foreground" Value="#FF00498A" />
        <Style.Triggers>
          <Trigger Property="IsEnabled" Value="False">
            <Setter Property="Foreground" Value="#FF808080" />
          </Trigger>
        </Style.Triggers>
      </Style>
    </ResourceDictionary>
  </FrameworkElement.Resources>
  <Grid Name="main_grid" SnapsToDevicePixels="True">
    <Grid.RowDefinitions>
      <RowDefinition Height="Auto" />
      <RowDefinition Height="*" />
      <RowDefinition Height="Auto" />
    </Grid.RowDefinitions>
    <Grid Name="vert_grid" Row="1" SnapsToDevicePixels="True">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <Grid Name="ctr_grid" Column="1" SnapsToDevicePixels="True" Margin="1">
        <Grid.RowDefinitions>
          <RowDefinition Height="Auto" />
          <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TabControl Name="tabs" Grid.Row="0" Height="23" SelectionChanged="tabs_SelectionChanged" />
        <Border Name="canvasborder" Grid.Row="1" BorderThickness="1" BorderBrush="{DynamicResource PnPopup.BorderBrush}" Background="{DynamicResource PnPopup.BackgroudBrush}">
          <ScrollViewer Name="scroll" VerticalAlignment="Top" HorizontalAlignment="Left" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <Canvas Name="canvas" />
          </ScrollViewer>
        </Border>
        <tree:ArchiveTreeFilterControl Name="ArchiveFilter" Grid.Row="1" TabIndex="1" Margin="4,0,0,0" PreviewMouseDoubleClick="ArchiveFilter_PreviewMouseDoubleClick" />
        <Border Name="tabbleborder" Grid.Row="1" BorderThickness="0" BorderBrush="#FFACACAC" SnapsToDevicePixels="True">
          <Grid SnapsToDevicePixels="True">
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto" />
              <RowDefinition Height="Auto" />
              <RowDefinition Height="Auto" />
              <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid Row="2">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
              </Grid.ColumnDefinitions>
              <StackPanel Name="sorting_btns_panel" Orientation="Horizontal" HorizontalAlignment="Left">
                <Button Name="MoveToStartButton" Width="23" Height="23" Margin="1" Style="{DynamicResource NavigationButtonStyle}" Click="MoveToStartClick">
                  <Path Name="btn1_path" Data="M32,0 L32,31.68 L4.4799995,16 M0,0 L4.1599998,0 L4.1599998,32 L0,32" Stretch="Uniform" Width="12" Height="12" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}">
                    <UIElement.RenderTransform>
                      <TransformGroup>
                        <TransformGroup.Children>
                          <RotateTransform Angle="90" />
                          <ScaleTransform ScaleX="1" ScaleY="1" />
                        </TransformGroup.Children>
                      </TransformGroup>
                    </UIElement.RenderTransform>
                  </Path>
                </Button>
                <Button Name="MoveOneUpButton" Width="23" Height="23" Margin="1" Style="{DynamicResource NavigationButtonStyle}" Click="MoveOneUpClick">
                  <Path Data="M0,0 L22.652,15.996998 L0,31.999996" Stretch="Uniform" Width="12" Height="12" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}">
                    <UIElement.RenderTransform>
                      <TransformGroup>
                        <TransformGroup.Children>
                          <RotateTransform Angle="270" />
                          <ScaleTransform ScaleX="1" ScaleY="1" />
                        </TransformGroup.Children>
                      </TransformGroup>
                    </UIElement.RenderTransform>
                  </Path>
                </Button>
                <Button Name="MoveOneDownButton" Width="23" Height="23" Margin="1" Style="{DynamicResource NavigationButtonStyle}" Click="MoveOneDownClick">
                  <Path Data="M0,0 L22.652,15.996998 L0,31.999996" Stretch="Uniform" Width="12" Height="12" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}">
                    <UIElement.RenderTransform>
                      <TransformGroup>
                        <TransformGroup.Children>
                          <RotateTransform Angle="90" />
                          <ScaleTransform ScaleX="1" ScaleY="1" />
                        </TransformGroup.Children>
                      </TransformGroup>
                    </UIElement.RenderTransform>
                  </Path>
                </Button>
                <Button Name="MoveToEndButton" Width="23" Height="23" Margin="1" Style="{DynamicResource NavigationButtonStyle}" Click="MoveToEndClick">
                  <Path Data="M32,0 L32,31.68 L4.4799995,16 M0,0 L4.1599998,0 L4.1599998,32 L0,32" Stretch="Uniform" Width="12" Height="12" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Button}}">
                    <UIElement.RenderTransform>
                      <TransformGroup>
                        <TransformGroup.Children>
                          <RotateTransform Angle="270" />
                          <ScaleTransform ScaleX="1" ScaleY="1" />
                        </TransformGroup.Children>
                      </TransformGroup>
                    </UIElement.RenderTransform>
                  </Path>
                </Button>
              </StackPanel>
              <StackPanel Name="filter_panel" Grid.Column="1" Height="30" Orientation="Horizontal" HorizontalAlignment="Right">
                <TextBox Name="filter_text" Height="23" Width="250" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,0,0" VerticalContentAlignment="Center" TextChanged="filter_text_TextChanged" />
                <Image Name="filter_image" Width="16" Height="16" Margin="2" MouseLeftButtonDown="filter_image_MouseLeftButtonDown" />
                <ComboBox Name="filter_combo" Height="23" Width="150" Margin="0,0,20,0" SelectionChanged="filter_combo_SelectionChanged" />
              </StackPanel>
            </Grid>
            <StackPanel Name="filter_panel1" Grid.Row="0" Grid.Column="1" Height="30" Orientation="Horizontal" HorizontalAlignment="Right">
              <TextBox Name="filter_text1" Height="23" Width="250" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,0,0" VerticalContentAlignment="Center" TextChanged="filter_text_TextChanged" />
              <Image Name="filter_image1" Width="16" Height="16" Margin="2" MouseLeftButtonDown="filter_image_MouseLeftButtonDown" />
              <ComboBox Name="filter_combo1" Height="23" Width="150" Margin="0,0,20,0" SelectionChanged="filter_combo_SelectionChanged" />
            </StackPanel>
            <StackPanel Name="filter_panel2" Grid.Row="1" Grid.Column="1" Height="30" Orientation="Horizontal" HorizontalAlignment="Right">
              <TextBox Name="filter_text2" Height="23" Width="250" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,0,0" VerticalContentAlignment="Center" TextChanged="filter_text_TextChanged" />
              <Image Name="filter_image2" Width="16" Height="16" Margin="2" MouseLeftButtonDown="filter_image_MouseLeftButtonDown" />
              <ComboBox Name="filter_combo2" Height="23" Width="150" Margin="0,0,20,0" SelectionChanged="filter_combo_SelectionChanged" />
            </StackPanel>
            <local:QuickTable1 x:Name="MyQuickTable_Type1" Grid.Row="3" Visibility="Collapsed" />
            <ContentControl Name="MyQuickTable_Type2" Grid.Row="3" />
          </Grid>
        </Border>
      </Grid>
    </Grid>
    <Grid Name="zone_btn" Row="0" SnapsToDevicePixels="True">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <Grid.RowDefinitions>
        <RowDefinition Height="Auto" />
        <RowDefinition Height="*" />
        <RowDefinition Height="Auto" />
      </Grid.RowDefinitions>
      <StackPanel Name="main_stack1" Grid.Column="0" Orientation="Horizontal" SnapsToDevicePixels="True">
        <StackPanel Name="stack1" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="B0" Content="B0" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B1" Content="B1" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B2" Content="B2" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B8" Content="B8" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
        <StackPanel Name="stack2" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="B4" Content="B4" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B6" Content="B6" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
        <StackPanel Name="stack3" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="B9" Content="B9" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B10" Content="B10" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
        <StackPanel Name="stack4" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="B11" Content="B11" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B12" Content="B12" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B13" Content="B13" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B14" Content="B14" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
      </StackPanel>
      <StackPanel Name="main_stack2" Grid.Column="2" Orientation="Horizontal" SnapsToDevicePixels="True">
        <StackPanel Name="stack5" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4">
          <Button Name="B15" Content="B15" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B7" Content="B7" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
        <StackPanel Name="stack6" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="B3" Content="B3" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
          <Button Name="B5" Content="B5" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
        <StackPanel Name="stack7" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="B16" Content="B16" Margin="1" SnapsToDevicePixels="True" Click="Button_Click" />
        </StackPanel>
        <StackPanel Name="expo" Visibility="Collapsed" HorizontalAlignment="Left" Orientation="Horizontal" Margin="4" SnapsToDevicePixels="True">
          <Button Name="Boo" Visibility="Collapsed" Content="Export" Margin="1" SnapsToDevicePixels="True" Click="Boo_Click" />
        </StackPanel>
      </StackPanel>
    </Grid>
  </Grid>
</Window>