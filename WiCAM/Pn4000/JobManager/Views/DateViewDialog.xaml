<UserControl x:Class="WiCAM.Pn4000.JobManager.DateViewDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dialogs="clr-namespace:MahApps.Metro.Controls.Dialogs;assembly=MahApps.Metro"
             xmlns:local="clr-namespace:WiCAM.Pn4000.JobManager"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:ribbon="http://schemas.microsoft.com/winfx/2006/xaml/presentation/ribbon"
             d:DataContext="{d:DesignInstance local:DialogsViewModel}"
             d:DesignHeight="60"
             d:DesignWidth="100"

             mc:Ignorable="d">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">

    <Button Margin="5,5,5,5" Command="{Binding ShowLeftFlyoutCommand}">FLYOUT</Button>
  </StackPanel>
</UserControl>