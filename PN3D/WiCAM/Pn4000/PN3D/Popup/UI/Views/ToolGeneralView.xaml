<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="WiCAM.Pn4000.PN3D.Popup.UI.Views.ToolGeneralView" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:popup="clr-namespace:WiCAM.Pn4000.Popup;assembly=WiCAM.Pn4000.Popup" xmlns:viewModel="clr-namespace:WiCAM.Pn4000.PN3D.Popup.UI.ViewModels" xmlns:loc2="clr-namespace:BendDataBase.WiCAM.Pn4000.pn4.Sym3D.BendSimulation.Converter;assembly=BendDataBase" Background="#00FFFFFF" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" xmlns:views="clr-namespace:WiCAM.Pn4000.PN3D.Popup.UI.Views">
  <FrameworkElement.Resources>
    <ResourceDictionary>
      <loc2:EnumToBoolConverter x:Key="enumToBool" />
      <loc2:BoolToIntConverter x:Key="boolToInt" />
      <Style x:Key="styleViewBoxCheck" TargetType="{x:Type Viewbox}">
        <Setter Property="Margin" Value="0,5,0,5" />
      </Style>
      <Style x:Key="styleListCheckBox" TargetType="{x:Type CheckBox}">
        <Setter Property="Width" Value="15" />
        <Setter Property="Height" Value="15" />
      </Style>
    </ResourceDictionary>
  </FrameworkElement.Resources>
  <Border BorderBrush="#FFA9A9A9" BorderThickness="1" CornerRadius="7.5">
    <Border.Background>
      <SolidColorBrush Color="WhiteSmoke" Opacity="0.8" />
    </Border.Background>
    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
      <Grid>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="1*" />
          <ColumnDefinition Width="1*" />
          <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Vertical" Visibility="Collapsed">
          <TextBlock Text="{DynamicResource l_popup.ToolGeneralView.PreferredBendingProcess}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontWeight="Bold" TextDecorations="Underline" Grid.ColumnSpan="2" Margin="0" />
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.ForLargeRadiusBends}" Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Column="0" />
            <Grid Column="1" Margin="0,2,0,26">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.AirBendProcess}" Height="30" VerticalContentAlignment="Center" GroupName="largeRadius" IsChecked="{Binding Path=ToolSettings.BendProcessForLargeRadiusBends, Converter={StaticResource enumToBool}, ConverterParameter=AirBendProcess}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.ShapeProcess}" Height="30" VerticalContentAlignment="Center" GroupName="largeRadius" IsChecked="{Binding Path=ToolSettings.BendProcessForLargeRadiusBends, Converter={StaticResource enumToBool}, ConverterParameter=ShapeProcess}" />
              <RadioButton Grid.Column="2" Content="{DynamicResource l_popup.ToolGeneralView.MultiStepAirBend}" Height="30" VerticalContentAlignment="Center" GroupName="largeRadius" IsChecked="{Binding Path=ToolSettings.BendProcessForLargeRadiusBends, Converter={StaticResource enumToBool}, ConverterParameter=MultiStepAirBendProcess}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.For90°Bends}" Style="{DynamicResource PnPopupLabelForTextBox}" />
            <Grid Column="1" Margin="0,2,0,26">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.AirBendProcess}" Height="30" VerticalContentAlignment="Center" GroupName="90degree" IsChecked="{Binding Path=ToolSettings.BendProcessFor90DegreeBends, Converter={StaticResource enumToBool}, ConverterParameter=AirBendProcess}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.ShapeProcess}" Height="30" VerticalContentAlignment="Center" GroupName="90degree" IsChecked="{Binding Path=ToolSettings.BendProcessFor90DegreeBends, Converter={StaticResource enumToBool}, ConverterParameter=ShapeProcess}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.ForOtherBends}" Style="{DynamicResource PnPopupLabelForTextBox}" />
            <Grid Column="1" Margin="0,2,0,26">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.AirBendProcess}" Height="30" VerticalContentAlignment="Center" GroupName="otherDends" IsChecked="{Binding Path=ToolSettings.BendProcessForOtherBends, Converter={StaticResource enumToBool}, ConverterParameter=AirBendProcess}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.ShapeProcess}" Height="30" VerticalContentAlignment="Center" GroupName="otherDends" IsChecked="{Binding Path=ToolSettings.BendProcessForOtherBends, Converter={StaticResource enumToBool}, ConverterParameter=ShapeProcess}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.UseAlways}" Style="{DynamicResource PnPopupLabelForTextBox}" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.IgnoreNonParallelEdges}" />
          </Grid>
        </StackPanel>
        <StackPanel Orientation="Vertical" Grid.Row="2" Margin="0,0,0,125" Visibility="Collapsed">
          <TextBlock Text="{DynamicResource l_popup.ToolGeneralView.PreferredQualityOfPart}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontWeight="Bold" TextDecorations="Underline" Grid.ColumnSpan="2" Margin="0" />
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.IgnorePunchMarks}" Style="{DynamicResource PnPopupLabelForTextBox}" Grid.Row="1" />
            <CheckBox Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.IgnorePunchMarks}" />
          </Grid>
        </StackPanel>
        <StackPanel Orientation="Vertical" Grid.Column="2" Grid.RowSpan="2" Visibility="Collapsed">
          <TextBlock Text="{DynamicResource l_popup.ToolGeneralView.PreferredToolAlignment}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontWeight="Bold" TextDecorations="Underline" Grid.ColumnSpan="2" Margin="0" />
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.PunchDieAlignment}" Style="{DynamicResource PnPopupLabelForTextBox}" />
            <Grid Column="1" Margin="0,2,0,26" Row="1" RowSpan="2">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.Left}" Height="30" VerticalContentAlignment="Center" GroupName="punchDie" IsChecked="{Binding Path=ToolSettings.PunchDieAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Left}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.Middle}" Height="30" VerticalContentAlignment="Center" GroupName="punchDie" IsChecked="{Binding Path=ToolSettings.PunchDieAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Middle}" />
              <RadioButton Grid.Column="2" Content="{DynamicResource l_popup.ToolGeneralView.Right}" Height="30" VerticalContentAlignment="Center" GroupName="punchDie" IsChecked="{Binding Path=ToolSettings.PunchDieAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Right}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.SynchronizePunchWithDie}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="0,2,0,0" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.SynchronizePunchWithDie}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.UpperAdapterAlignment}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="0,2,0,26" Grid.RowSpan="2" />
            <Grid Column="1" Margin="0,2,0,26" RowSpan="2">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.Left}" Height="30" VerticalContentAlignment="Center" GroupName="upperAdapter" IsChecked="{Binding Path=ToolSettings.UpperAdapterAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Left}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.Middle}" Height="30" VerticalContentAlignment="Center" GroupName="upperAdapter" IsChecked="{Binding Path=ToolSettings.UpperAdapterAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Middle}" />
              <RadioButton Grid.Column="2" Content="{DynamicResource l_popup.ToolGeneralView.Right}" Height="30" VerticalContentAlignment="Center" GroupName="upperAdapter" IsChecked="{Binding Path=ToolSettings.UpperAdapterAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Right}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.LowerAdapterAlignment}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="0" />
            <Grid Column="1" Margin="0,2,0,26" Row="4" RowSpan="2">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.Left}" Height="30" VerticalContentAlignment="Center" GroupName="lowerAdapter" IsChecked="{Binding Path=ToolSettings.LowerAdapterAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Left}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.Middle}" Height="30" VerticalContentAlignment="Center" GroupName="lowerAdapter" IsChecked="{Binding Path=ToolSettings.LowerAdapterAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Middle}" />
              <RadioButton Grid.Column="2" Content="{DynamicResource l_popup.ToolGeneralView.Right}" Height="30" VerticalContentAlignment="Center" GroupName="lowerAdapter" IsChecked="{Binding Path=ToolSettings.LowerAdapterAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Right}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.ToolSetupAlignment}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="0,26" Grid.RowSpan="3" />
            <Grid Column="1" Margin="0,2,0,26" Row="5" RowSpan="2">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
              </Grid.ColumnDefinitions>
              <RadioButton Grid.Column="0" Content="{DynamicResource l_popup.ToolGeneralView.Left}" Height="30" VerticalContentAlignment="Center" GroupName="toolSetup" IsChecked="{Binding Path=ToolSettings.ToolSetupAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Left}" />
              <RadioButton Grid.Column="1" Content="{DynamicResource l_popup.ToolGeneralView.Middle}" Height="30" VerticalContentAlignment="Center" GroupName="toolSetup" IsChecked="{Binding Path=ToolSettings.ToolSetupAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Middle}" />
              <RadioButton Grid.Column="2" Content="{DynamicResource l_popup.ToolGeneralView.Right}" Height="30" VerticalContentAlignment="Center" GroupName="toolSetup" IsChecked="{Binding Path=ToolSettings.ToolSetupAlignment, Converter={StaticResource enumToBool}, ConverterParameter=Right}" />
            </Grid>
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.GapBetweenToolSetups}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="0,0,0,26" Grid.RowSpan="2" />
            <popup:TextBoxInt Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="1" Text="{Binding ToolSettings.GapBetweenToolSetups}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.OffsetBetweenSetups}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="0,0.125,0,-0.125" />
            <TextBox Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="1" Text="{Binding ToolSettings.SecurityOffsetBetweenSetups}" />
          </Grid>
        </StackPanel>
        <StackPanel Orientation="Vertical" Grid.Row="1" Visibility="Collapsed">
          <TextBlock Text="{DynamicResource l_popup.ToolGeneralView.PreferredToolSelection}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontWeight="Bold" TextDecorations="Underline" Grid.ColumnSpan="2" Margin="0" />
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.PrefPunchOverLength}" Style="{DynamicResource PnPopupLabelForTextBox}" />
            <TextBox Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="1" Text="{Binding ToolSettings.PrefPunchOverlength}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.PrefDieOverLength}" Style="{DynamicResource PnPopupLabelForTextBox}" />
            <TextBox Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="1" Text="{Binding ToolSettings.PrefDieOverlength}" />
          </Grid>
        </StackPanel>
        <StackPanel Orientation="Vertical" Grid.Column="1" Grid.RowSpan="2">
          <TextBlock Text="{DynamicResource l_popup.ToolGeneralView.ToolVaidationOptions}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontWeight="Bold" TextDecorations="Underline" Grid.ColumnSpan="2" Margin="0" />
          <Grid Margin="2,2,2,20">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.MaxGapToolsAndBend}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.MaxGapToolsAndBendTt}" Grid.Column="0" />
            <TextBox Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="1" Width="50px" Margin="0 3 0 3" HorizontalAlignment="Center" Text="{Binding ToolSettings.MaxGapToolsToBend}" />
          </Grid>
          <Grid Margin="2,2,2,20">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.MaxGapLegsAndBend}" ToolTip="{DynamicResource l_popup.ToolGeneralView.MaxGapLegsAndBendTt}" Grid.Column="0" />
            <TextBox Grid.Column="1" Width="50px" Margin="0 3 0 3" HorizontalAlignment="Center" Text="{Binding ToolSettings.MaxGapLegCoverageToBend}" />
          </Grid>
          <TextBlock Text="{DynamicResource l_popup.ToolGeneralView.PreferredOptimizationAtToolSelection}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" FontWeight="Bold" TextDecorations="Underline" Grid.ColumnSpan="2" Margin="0" />
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.ToolCollisionMargin}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.ToolCollisionMarginTooltip}" Grid.ColumnSpan="2" />
            <TextBox Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="1" Width="50px" Margin="0 3 0 3" HorizontalAlignment="Center" ToolTip="{DynamicResource l_popup.ToolGeneralView.ToolCollisionMarginValueRangeTooltip}" Text="{Binding ToolSettings.PreferredToolCollisionMargin}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.BendMargin}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.BendMarginTooltip}" Grid.ColumnSpan="3" />
            <TextBox Style="{DynamicResource PnPopupTextBoxStyle}" Grid.Column="3" Width="50Px" Margin="0 3 0 3" HorizontalAlignment="Center" ToolTip="{DynamicResource l_popup.ToolGeneralView.BendMarginValueRangeTooltip}" Text="{Binding ToolSettings.PreferredBendMargin}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.FlipTools}" Style="{DynamicResource PnPopupLabelForTextBox}" Margin="3,2,0,2" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.CheckOverlapPunchWithBend}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.FillGaps}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.FillGapsTooltip}" Margin="3,2,0,2" Grid.RowSpan="2" IsEnabled="True" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsEnabled="True" IsChecked="{Binding ToolSettings.FillGaps}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.SameProfile}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.SameProfileTooltip}" Margin="3,2,0,2" IsEnabled="True" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsEnabled="True" IsChecked="{Binding ToolSettings.UseSameProfileForAllBends}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.LastHopeAllowDifferentWorkingHeights}" ToolTip="{DynamicResource l_popup.ToolGeneralView.LastHopeAllowDifferentWorkingHeightsTt}" Margin="3,2,0,2" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.LastHopeAllowDifferentWorkingHeights}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.SelectHeelToolsInCalculation}" ToolTip="{DynamicResource l_popup.ToolGeneralView.SelectHeelToolsInCalculationToolTip}" Margin="3,2,0,2" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.AllowHeelToolsInSetup}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Content="{DynamicResource l_popup.ToolGeneralView.AllowFlipToolsInCalculation}" ToolTip="{DynamicResource l_popup.ToolGeneralView.AllowFlipToolsInCalculationTt}" Margin="3,2,0,2" />
            <CheckBox Grid.Column="1" Style="{StaticResource styleListCheckBox}" IsChecked="{Binding ToolSettings.AllowFlipTools}" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>
            <Label Grid.Column="0" HorizontalAlignment="Center" Content="{DynamicResource l_popup.ToolGeneralView.Optimization}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.OptimizationTooltip}" IsEnabled="True" Margin="3,2,0,2" />
          </Grid>
          <Grid Margin="2">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="140" />
              <ColumnDefinition Width="1*" />
              <ColumnDefinition Width="140" />
            </Grid.ColumnDefinitions>
            <Label Grid.Column="0" HorizontalAlignment="Center" Content="{DynamicResource l_popup.ToolGeneralView.OptimizationTotalLength}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.OptimizationTotalLengthTooltip}" IsEnabled="True" Margin="3,2,0,2" />
            <Slider Grid.Column="1" IsEnabled="True" Margin="3,2,0,2" Minimum="0.0" Maximum="1.0" Value="{Binding ToolSettings.ToolStationOptimizationParam1}" />
            <Label Grid.Column="2" HorizontalAlignment="Center" Content="{DynamicResource l_popup.ToolGeneralView.OptimizationToolCoverage}" Style="{DynamicResource PnPopupLabelForTextBox}" ToolTip="{DynamicResource l_popup.ToolGeneralView.OptimizationToolCoverageTooltip}" IsEnabled="True" Margin="3,2,0,2" />
          </Grid>
        </StackPanel>
      </Grid>
    </ScrollViewer>
  </Border>
</UserControl>